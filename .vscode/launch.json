    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Yarn start",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "start-env",
                "--source",
                "tesla-website"
            ],
            "port": 5858,
            "cwd": "${workspaceRoot}",
            "timeout": 10000,
            "skipFiles": [
                "${workspaceFolder}/<node_internals>/**",
                "${workspaceFolder}/node_modules/**"
                 ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Yarn test",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "test"
            ],
            "port": 5858,
            "cwd": "${workspaceRoot}",
            "timeout": 10000,
            "skipFiles": [
                "${workspaceFolder}/<node_internals>/**",
                "${workspaceFolder}/node_modules/**"]
        }
    ]
}